{
  "name": "password-tester",
  "version": "1.2.2",
  "description": "Test password/phrases to ensure strong entropy and no reuse from a password breach, based on the latest guidance.",
  "files": [
    "dist"
  ],
  "main": "./dist/password-tester.umd.js",
  "module": "./dist/password-tester.es.js",
  "exports": {
    ".": {
      "require": "./dist/password-tester.umd.js",
      "import": "./dist/password-tester.es.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "format": "biome format --write ./lib",
    "lint": "biome lint ./lib",
    "fix": "biome check --apply ./lib"
  },
  "author": "David Sint",
  "license": "(MIT OR Apache-2.0)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidSint/password-tester.git"
  },
  "keywords": [
    "password",
    "security",
    "devsecops",
    "validation",
    "validator",
    "test",
    "password tester",
    "checker",
    "strength",
    "password strength",
    "passphrase",
    "registration",
    "change password",
    "have i been pwned"
  ],
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@vitest/coverage-v8": "^1.2.2",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2"
  }
}
